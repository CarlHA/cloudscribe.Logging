{
    "version": "1.0.0-beta6",
    "buildOptions": {
        "emitEntryPoint": true
    },
    "description": "Entity Framework 7 implementation of cloudscribe ILogRepository",
    "authors": [ "Joe Audette" ],
    "packOptions": {
        "owners": [ "Source Tree Solutions" ],
        "tags": [ "cloudscribe", "repositories", "logging" ],
        "projectUrl": "https://github.com/joeaudette/cloudscribe.Logging",
        "licenseUrl": "http://www.apache.org/licenses/LICENSE-2.0",
        "copyright": "2015 Source Tree Solutions",
        "iconUrl": "https://raw.githubusercontent.com/joeaudette/cloudscribe/master/cloudscribe-icon-32.png",
        "requireLicenseAcceptance": false,
        "releaseNotes": ""
    },

    "dependencies": {
        
        "cloudscribe.Logging.Web": "1.0.0-*",
		"Microsoft.AspNetCore.Server.IISIntegration": "1.0.0-rc2-final",
         "Microsoft.AspNetCore.Server.Kestrel": "1.0.0-rc2-final",
		 "Microsoft.AspNetCore.Mvc": "1.0.0-rc2-final",
		 "Microsoft.Extensions.Configuration.Json": "1.0.0-rc2-final",
        "Microsoft.EntityFrameworkCore.SqlServer": "1.0.0-rc2-final",
        "Microsoft.EntityFrameworkCore.Tools": {
            "version": "1.0.0-preview1-final",
            "type": "build"
        }

    },

    "tools": {
        "Microsoft.EntityFrameworkCore.Tools": {
            "version": "1.0.0-preview1-final",
            "imports": [
                "portable-net45+win8+dnxcore50",
                "portable-net45+win8"
            ]
        }
    },

    "frameworks": {
        "netcoreapp1.0": {
            "imports": [ "portable-net451+win8" ],
            "buildOptions": {
                "emitEntryPoint": true
            },
            "dependencies": {
                "Microsoft.NETCore.App": {
                    "type": "platform",
                    "version": "1.0.0-*"
                }
            }
        }
    }
}
